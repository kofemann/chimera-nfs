<?xml version="1.0" encoding="UTF-8"?>
<hazelcast
        xsi:schemaLocation="http://www.hazelcast.com/schema/config http://www.hazelcast.com/schema/config/hazelcast-config-3.8.xsd"
        xmlns="http://www.hazelcast.com/schema/config"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <!-- properties will be passed via -D... JAVA_OPTS -->
    <!-- see hazelcast.yml -->
    <management-center enabled="true" update-interval="3">
        ${hazelcast.mancenter.url}
    </management-center>

    <cache name="open-stateid">
         <key-type class-name="[B"/>
         <value-type class-name="[B"/>
         <in-memory-format>BINARY</in-memory-format>
         <read-through>false</read-through>
         <write-through>false</write-through>
         <statistics-enabled>true</statistics-enabled>
         <management-enabled>true</management-enabled>
     </cache>

</hazelcast>
